<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">

    <type name="Magento\NotifierApi\Api\ChannelRepositoryInterface">
        <plugin sortOrder="10" name="msp_queue_save_extension_attributes_for_channel"
                type="Magento\NotifierAsync\Plugin\SaveExtensionAttributesForChannel"/>
        <plugin sortOrder="10" name="msp_queue_add_extension_attributes_to_channel"
                type="Magento\NotifierAsync\Plugin\AddExtensionAttributesToChannel"/>
    </type>

    <type name="Magento\NotifierApi\Api\SendMessageInterface">
        <plugin sortOrder="1" name="mSPNotifierQueueSendMessageInterface"
                type="Magento\NotifierAsync\Plugin\SendMessageAsynchronously"/>
    </type>
</config>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\ReCaptchaAdminUi\Model\AdminConfigInterface"
                type="Magento\ReCaptchaAdminUi\Model\AdminConfig"/>

    <type name="Magento\Config\Model\Config\TypePool">
        <arguments>
            <argument name="sensitive" xsi:type="array">
                <item name="recaptcha/backend/public_key" xsi:type="string">1</item>
                <item name="recaptcha/backend/private_key" xsi:type="string">1</item>
            </argument>
            <argument name="environment" xsi:type="array">
                <item name="recaptcha/backend/public_key" xsi:type="string">1</item>
                <item name="recaptcha/backend/private_key" xsi:type="string">1</item>
            </argument>
        </arguments>
    </type>
</config>

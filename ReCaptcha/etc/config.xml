<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:module:Magento_Store:etc/config.xsd">
    <default>
        <recaptcha>
            <general>
                <type>recaptcha_v3</type>
            </general>
            <backend>
                <enabled>0</enabled>
                <size>normal</size>
                <theme>light</theme>
                <min_score>0.9</min_score>
            </backend>
            <frontend>
                <enabled>0</enabled>
                <size>normal</size>
                <position>inline</position>
                <theme>light</theme>
                <lang></lang>
                <enabled_login>1</enabled_login>
                <enabled_forgot>1</enabled_forgot>
                <enabled_contact>1</enabled_contact>
                <enabled_create>1</enabled_create>
                <enabled_review>1</enabled_review>
                <enabled_newsletter>1</enabled_newsletter>
                <enabled_sendfriend>1</enabled_sendfriend>
                <min_score>0.6</min_score>
            </frontend>
        </recaptcha>
    </default>
</config>
